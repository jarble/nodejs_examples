addFunction(generateFile);
s.requiresTheFunctions = false;
s.isDefined = true;

function generateFile(fileName, fileText) {
    var fs = require('fs');
    fs.writeFile(fileName, fileText, function (err) {
        if (err) {
            console.log(err);
        }
    });
}
